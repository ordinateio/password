{
  "name": "@ordinateio/password",
  "version": "2.0.0",
  "description": "Generates strong passwords.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "author": "Ordinate IO (https://github.com/ordinateio)",
  "license": "MIT",
  "homepage": "https://github.com/ordinateio/password",
  "bugs": {
    "url": "https://github.com/ordinateio/password/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ordinateio/password.git"
  },
  "keywords": [
    "front-end",
    "back-end",
    "typescript"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:dist",
    "build:dist": "tsc --project tsconfig.json",
    "test": "jest",
    "clean": "rimraf .cache dist",
    "npm:pack": "npm run build && npm pack",
    "npm:publish": "npm run build && npm publish --access public",
    "npm:install:lock:only": "npm install --package-lock-only"
  },
  "jest": {
    "cacheDirectory": ".cache/jest",
    "coverageDirectory": ".cache/jest/coverage",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^27",
    "@types/node": "^16",
    "jest": "^27",
    "rimraf": "^3",
    "ts-jest": "^27",
    "typescript": "^4"
  }
}
